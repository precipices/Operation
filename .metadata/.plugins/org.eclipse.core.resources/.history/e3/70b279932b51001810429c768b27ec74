package com.operation.myComponent;

import java.awt.Color;

import javax.swing.Action;
import javax.swing.Icon;
import javax.swing.JRadioButton;

public class BackRadioButton extends JRadioButton{
	private void init() {
		this.setOpaque(false);
		this.setContentAreaFilled(false);
//		this.setBackground(Color.YELLOW);
	}

	public BackRadioButton() {
		super();
		init();
	}

	public BackRadioButton(Action arg0) {
		super(arg0);
		init();
	}

	public BackRadioButton(Icon arg0, boolean arg1) {
		super(arg0, arg1);
		init();
	}

	public BackRadioButton(Icon arg0) {
		super(arg0);
		init();
	}

	public BackRadioButton(String arg0, boolean arg1) {
		super(arg0, arg1);
		init();
	}

	public BackRadioButton(String arg0, Icon arg1, boolean arg2) {
		super(arg0, arg1, arg2);
		init();
	}

	public BackRadioButton(String text, Icon icon) {
		super(text, icon);
		init();
	}

	public BackRadioButton(String text) {
		super(text);
		init();
	}

}
